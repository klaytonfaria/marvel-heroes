{
  "name": "marvel-heroes",
  "version": "1.0.0",
  "description": "Marvel heroes app",
  "author": "Klayton Faria <klayton.souza@gmail.com> (klaytonfaria.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/klaytonfaria/marvel-heroes/issues"
  },
  "homepage": "https://github.com/klaytonfaria/marvel-heroes#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/klaytonfaria/marvel-heroes.git"
  },
  "keywords": [
    "marvel",
    "heroes"
  ],
  "scripts": {
    "start": "webpack-dev-server --progress --hot",
    "prod": "webpack-dev-server -p",
    "build": "webpack -p",
    "lint": "eslint ./client ./webpack.config.js -f table || true",
    "test": "npm run lint",
    "precommit": "validate-commit-msg",
    "release": "semantic-release pre && semantic-release post"
  },
  "dependencies": {
    "autoprefixer": "^6.5.4",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.9",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4 ",
    "html-webpack-plugin": "^2.24.1",
    "jsdom": "^9.9.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "semantic-release": "^6.3.5",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta",
    "webpack-dev-server": "^2.1.0-beta.12",
    "webpack-hot-middleware": "2.13.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^3.12.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.7.1",
    "husky": "^0.12.0",
    "node-sass": "^4.1.1",
    "postcss-loader": "^1.2.1",
    "sass-loader": "^4.1.1",
    "semantic-release": "^6.3.5",
    "validate-commit-message": "^3.0.1"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ]
    }
  }
}
