{
  "name": "marvel-heroes",
  "version": "0.0.0-development",
  "description": "Marvel heroes app",
  "author": "Klayton Faria <klayton.souza+npm@gmail.com> (klaytonfaria.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/klaytonfaria/marvel-heroes/issues"
  },
  "homepage": "https://github.com/klaytonfaria/marvel-heroes#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/klaytonfaria/marvel-heroes.git"
  },
  "keywords": [
    "marvel",
    "heroes"
  ],
  "scripts": {
    "start": "webpack-dev-server",
    "dev": "webpack-dashboard -t zstore -c lightblue -- webpack-dev-server -p",
    "prod": "webpack-dev-server -p",
    "build": "webpack -p",
    "lint": "eslint ./client ./webpack.config.js -f table || true",
    "test": "npm run lint",
    "precommit": "validate-commit-msg",
    "release": "semantic-release pre && semantic-release post"
  },
  "dependencies": {
    "jsdom": "9.8.3",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-router": "3.0.0",
    "webpack": "^2.1.0-beta",
    "webpack-dev-server": "^2.1.0-beta.12",
    "webpack-hot-middleware": "2.13.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "husky": "^0.11.9",
    "semantic-release": "^6.3.2",
    "validate-commit-message": "^3.0.1",
    "webpack-dashboard": "^0.2.0"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ]
    }
  }
}
