{
  "name": "marvel-heroes",
  "version": "1.0.0",
  "description": "Marvel heroes app",
  "author": "Klayton Faria <klayton.souza@gmail.com> (klaytonfaria.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/klaytonfaria/marvel-heroes/issues"
  },
  "homepage": "https://github.com/klaytonfaria/marvel-heroes#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/klaytonfaria/marvel-heroes.git"
  },
  "keywords": [
    "marvel",
    "heroes"
  ],
  "scripts": {
    "start": "webpack-dev-server",
    "dev": "./node_modules/webpack-dashboard/bin/webpack-dashboard.js -t zstore -c lightblue -- webpack-dev-server -p",
    "prod": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js -p",
    "build": "./node_modules/webpack/bin/webpack.js -p",
    "lint": "eslint ./client ./webpack.config.js -f table || true",
    "test": "npm run lint",
    "precommit": "npm test && ./node_modules/validate-commit-message/bin/validate-commit-msg.js",
    "release": "./node_modules/semantic-release/bin/semantic-release.js pre && ./node_modules/semantic-release/bin/semantic-release.js post"
  },
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.9",
    "jsdom": "9.8.3",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-router": "^3.0.0",
    "webpack": "^2.1.0-beta",
    "webpack-dev-server": "^2.1.0-beta.12",
    "webpack-hot-middleware": "2.13.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "husky": "^0.11.9",
    "semantic-release": "^6.3.2",
    "validate-commit-message": "^3.0.1",
    "webpack-dashboard": "^0.2.0"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ]
    }
  }
}
